<meta name="viewport" content="width=device-width, initial-scale=1">

{% if UserConfig.bootstrapTheme and UserConfig.bootstrapTheme != 'default' %}
	{% set startupapi_bootstrap_css = UserConfig.USERSROOTURL ~ '/bootswatch/' ~ UserConfig.bootstrapTheme ~ '/bootstrap.min.css' %}
{% else %}
	{% set startupapi_bootstrap_css = UserConfig.USERSROOTURL ~ '/bootstrap3/css/bootstrap.min.css' %}
{% endif %}

<link{% if UserConfig.DEVMODE %} id="startupapi-dev-theme-switcher"{% endif %} href="{{startupapi_bootstrap_css}}" rel="stylesheet">

<script src="{{ UserConfig.USERSROOTURL }}/jquery-1.11.1.min.js"></script>
<script src="{{ UserConfig.USERSROOTURL }}/bootstrap3/js/bootstrap.min.js"></script>
<script src="{{ UserConfig.USERSROOTURL }}/startupapi.js"></script>
<link rel="stylesheet" type="text/css" href="{{ UserConfig.USERSROOTURL }}/themes/{{ UserConfig.theme }}/startupapi.css">