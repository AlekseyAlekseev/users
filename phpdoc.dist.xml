<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>StartupAPI</title>
	<parser>
		<target>docs/phpdoc</target>
	</parser>
	<transformer>
		<target>docs/phpdoc</target>
	</transformer>
	<transformations>
		<template name="responsive" />
	</transformations>
	<files>
		<directory>.</directory>
		<ignore>oauth-php/*</ignore>
		<ignore>modules/facebook/facebook-php-sdk/*</ignore>
		<ignore>dbupgrade/*</ignore>
		<ignore>admin/swfobject/*</ignore>
		<ignore>docs/*</ignore>
		<ignore>twig/*</ignore>
		<ignore>cache/*</ignore>
		<ignore>phptidy/*</ignore>
		<ignore>MrClay_CookieStorage.php</ignore>
		<ignore>swiftmailer/*</ignore>
		<ignore>php-bootstrap/*</ignore>
	</files>
</phpdoc>
